{
  "name": "wordbots",
  "version": "0.12.0-alpha",
  "description": "A card-and-board game that showcases card game semantic parsing",
  "license": "MIT",
  "engines": {
    "node": "8.x"
  },
  "browserslist": [
    "last 4 versions",
    "chrome >= 54",
    "not ie <= 11"
  ],
  "scripts": {
    "analyze": "npm run prebuild && yarn run webpack -p --config webpack.build.config.js --progress --devtool source-map --display minimal && source-map-explorer dist/bundle.js",
    "build": "npm run clean && npm run prebuild && set NODE_ENV=production && webpack -p --config webpack.build.config.js --progress --colors --profile",
    "build:stats": "npm run prebuild && node_modules/.bin/webpack -p --config webpack.build.config.js --json > stats.json",
    "clean": "node bin/clean.js dist",
    "heroku-postbuild": "npm run clean && npm run prebuild && set NODE_ENV=production && webpack -p --config webpack.build.config.js --progress --colors --profile",
    "prebuild": "sed 's/export default webpackConfig;/module.exports = webpackConfig;/' <webpack.config.js >webpack.build.config.js",
    "slate": "node bin/clean.js node_modules",
    "start": "set NODE_ENV=development && ts-node src/server/index.ts --colors --profile",
    "start-prod": "set NODE_ENV=production && ts-node src/server/index.ts --progress --colors --profile",
    "test": "run-s test:lint test:types test:unit",
    "test:lint": "run-s test:lint:es test:lint:ts",
    "test:lint:es": "eslint src test",
    "test:lint:ts": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "test:types": "tsc --skipLibCheck --noEmit",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wordbots/wordbots-core.git"
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@material-ui/core": "3.9.1",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@types/react-slick": "^0.23.3",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.1",
    "better-react-spinkit": "^2.0.4",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^1.0.0",
    "d3": "^3.5.16",
    "express": "^4.16.1",
    "fast-json-patch": "^2.0.5",
    "file-loader": "^1.1.11",
    "firebase": "^5.0.2",
    "image-webpack-loader": "^4.0.0",
    "json-loader": "^0.5.7",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "merge": "1.2.1",
    "moment": "^2.22.2",
    "object.values": "^1.1.0",
    "prop-types": "^15.6.1",
    "qs": "^6.6.0",
    "rc-slider": "^8.6.1",
    "react": "16.7.0",
    "react-bar-chart": "^0.1.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "16.7.0",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-iframe": "^1.8.0",
    "react-markdown": "^4.0.0",
    "react-popover": "^0.5.4",
    "react-redux": "^6.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.0",
    "react-sound": "1.2.0",
    "react-textfit": "^1.0.1",
