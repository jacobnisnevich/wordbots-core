    "@types/express": "^4.16.0",
    "@types/history": "^4.6.2",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.109",
    "@types/material-ui": "^0.21.5",
    "@types/node": "^11.13.4",
    "@types/qs": "^6.5.2",
    "@types/rc-slider": "^8.6.0",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.0.8",
    "@types/react-helmet": "^5.0.7",
    "@types/react-popover": "^0.5.3",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-tooltip": "^3.9.0",
    "@types/redux-devtools": "^3.0.46",
    "@types/redux-devtools-dock-monitor": "^1.1.33",
    "@types/redux-devtools-log-monitor": "^1.0.34",
    "@types/screenfull": "^4.1.0",
    "@types/seed-random": "^2.2.0",
    "@types/shuffle-array": "^0.0.28",
    "@types/webpack": "^4.0.0",
    "@types/whatwg-fetch": "^0.0.33",
    "@types/ws": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "csstype": "^2.5.2",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "^5.0.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.9.0",
