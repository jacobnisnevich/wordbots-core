    "react-transition-group": "^2.2.1",
    "react-web-notification": "^0.5.0",
    "redux": "^4.0.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "3.0.6",
    "repng": "1.0.1",
    "screenfull": "4.1.0",
    "seed-random": "^2.2.0",
    "seed-shuffle": "^1.0.0",
    "shuffle-array": "^1.0.1",
    "soundmanager2": "^2.97.20170602",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.23.1",
    "substitute-loader": "^1.0.0",
    "trianglify": "1.2.0",
    "ts-node": "^8.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.8",
    "whatwg-fetch": "^2.0.3",
