    "expect": "^23.0.1",
    "ignore-styles": "^5.0.1",
    "jest": "24.7.1",
    "jsdom-no-contextify": "3.1.0",
    "mocha": "^6.1.0",
    "mock-socket": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "16.7.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "ts-jest": "24.0.2",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "^2.9.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-middleware": "2.0.4",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "optionalDependencies": {
    "canvas-prebuilt": "^1.6.0"
  },
  "resolutions": {
    "repng/babel-loader": "^8.0.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test",
      "src/common/components/hexgrid/",
      "src/common/components/Sprite.js"
    ],
    "globals": {
      "_": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./test/beforeTest.ts"
    ],
    "testRegex": "(\\.|/)(test|spec)\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    }
  },
  "greenkeeper": {
    "ignore": [
      "css-loader",
      "d3",
      "file-loader",
      "webpack",
      "webpack-dev-middleware"
    ]
  }
}
